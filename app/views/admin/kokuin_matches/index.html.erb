<p id="notice"><%= notice %></p>

<h1>Listing Kokuin Matches</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @kokuin_matches.each do |kokuin_match| %>
      <tr>
        <td><%= link_to 'Show', kokuin_match %></td>
        <td><%= link_to 'Edit', edit_kokuin_match_path(kokuin_match) %></td>
        <td><%= link_to 'Destroy', kokuin_match, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Kokuin match', new_kokuin_match_path %>
