<div class="row">
  <div class="large-4 columns"><h2>Listing Photos</h2></div>
  <div class="large-4 columns"><%= link_to 'New Photo', new_admin_photo_path, {class: "button tiny radius"} %></small></div>
    <div class="large-4 columns"></div>

  </div>
<%= paginate @photos %>
<table>
  <thead>
    <tr>
      <th>id</th>
      <th>thumb</th>
      <th>photo</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @photos.each do |photo| %>
      <tr>
        <td><%= photo.id %></td>
        <td><%= image_tag photo.thumbnail %></td>
        <td><%= photo.fullpicture %></td>
        <td><%= link_to 'Show', admin_photo_path(photo) %></td>
        <td><%= link_to 'Edit', edit_admin_photo_path(photo) %></td>
        <td><%= link_to 'Destroy', admin_photo_path(photo), method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

